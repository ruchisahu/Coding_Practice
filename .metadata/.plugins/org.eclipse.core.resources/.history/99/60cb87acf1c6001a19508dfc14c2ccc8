 //* Path(Graph G,int s)
 //* boolean hasPathTo(int v)  //path from  s to v
 //* Iterable<Integer> pathTo(int v)  path from s to v
 
public class DepthfirstPath {

	private boolean[] marked;
	private int[] edgeTo;
	private int s;
	
	DepthfirstPath(Graph G, int s){
		marked=new boolean[G.V()];
		edgeTo=new int[G.V()];
	}
